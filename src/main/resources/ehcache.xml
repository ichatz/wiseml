           overflowToDisk="false"
           diskPersistent="false"
           timeToIdleSeconds="0"
           timeToLiveSeconds="0"
           memoryStoreEvictionPolicy="LRU"
           statistics="true"/>

    <cache name="nodeListsCache"
           eternal="false"
           maxElementsInMemory="400"
           overflowToDisk="false"
           diskPersistent="false"
           timeToIdleSeconds="0"
           timeToLiveSeconds="0"
           memoryStoreEvictionPolicy="LRU"
           statistics="true"/>
    <cache name="testCache"
           eternal="false"
           maxElementsInMemory="400"
           overflowToDisk="false"
           diskPersistent="false"
           timeToIdleSeconds="0"
           timeToLiveSeconds="0"
           memoryStoreEvictionPolicy="LRU"
           statistics="true"/>
    <cache name="eu.wisebed.wisedb.model.Testbed"
           maxElementsInMemory="5"
           eternal="true"
