<?xml version="1.0" encoding="UTF-8"?>
<ehcache>

    <defaultCache
            maxElementsInMemory="100000"
            eternal="false"
            overflowToDisk="true"
            timeToLiveSeconds="60"
            timeToIdleSeconds="30"/>

    <cache name="eu.wisebed.wiseml.model.setup.Node"
           maxElementsInMemory="100"
           eternal="false"
           timeToIdleSeconds="300"
           timeToLiveSeconds="600"
           memoryStoreEvictionPolicy="LRU"
           statistics="true"
           overflowToDisk="true"
            />
    <cache name="eu.wisebed.wiseml.model.setup.Node.readings"
           maxElementsInMemory="1000"
           eternal="false"
           timeToIdleSeconds="300"
           timeToLiveSeconds="600"
           memoryStoreEvictionPolicy="LRU"
           statistics="true"
           overflowToDisk="true"
            />
    <cache
            name="org.hibernate.cache.StandardQueryCache"
            maxElementsInMemory="50"
            eternal="false"
            timeToLiveSeconds="120"
            memoryStoreEvictionPolicy="LRU"
            statistics="true"
            overflowToDisk="true"/>
</ehcache>